/* pages/assets/assets.wxss */
page {
  height: 100%;
  background: #f1f1f1;
}
.nav {
  width: 100%;
  height: 45rpx;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}

.nav-title {
  width: 100%;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  position: absolute;
  bottom: 30rpx;
  left: 0;
  z-index: 10;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}
.return {
  width: 30rpx;
  height: 45rpx;
  position: absolute;
  left: 32rpx;
}
.back {
  width: 750rpx;
  height: 128rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.gao {
  width: 750rpx;
  height: 80rpx;
  background: #FFFFFF;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
.qu {
margin-left: 50rpx;
}
.price {
margin-right: 58rpx;
}
.song {
margin-left: 54rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.shijian {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #7A7A7A;
}
.prices {
font-size: 26rpx;
font-family: PingFang SC;
font-weight: 500;
color: #D72720;
